package lab1;

import java.util.Scanner;

public class Volume2 {
	public static void main(String[] args) {
		float c;
		double f;
		 System.out.println("fahrenhenit");
		 Scanner sc=new Scanner(System.in);
		 c=sc.nextFloat();
f=c*9/5+32;
System.out.println("converted to celsius:" +f);
	 
	}

}
